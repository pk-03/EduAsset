  <body class="bg-white">
   <%- include ./partials/header %>

       <!-- <h1 class="text-center text-info">This is the register page.</h1> -->
<form action="/login" method="post"  class="border mt-5 containerJI p-5" >
    <%- include ./partials/messages %>
    <h2 class="text-capitalize text-center">login here!</h2>
    <hr>
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" name="email" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email" required autocomplete="off">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" name="password" placeholder="Password" required>
  </div>
  <button type="submit" class="btn btn-info">login</button>
  <button class="btn btn-danger text-capitalize"><a class="text-white" href="/auth/google">Login with Google+</a></button>
  <div class="d-block float-right">
  <small class="px-2">OR Do not have an account!</small>
  <button class="btn btn-info"><a class="text-light" href="/signup">Sign up</a></button>
   </div>
</form>



